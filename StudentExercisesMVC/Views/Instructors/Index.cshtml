@model IEnumerable<StudentExercisesMVC.Models.Instructor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstFirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstLastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstSlackHandle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstCohort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstSpeciality)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstSlackHandle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstCohort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstSpeciality)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
